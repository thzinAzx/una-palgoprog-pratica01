programa {
  funcao inicio() {
    cadeia nomeCidade, cidadeEscola

    escreva("Infome o nome da sua cidade: ")
    leia(nomeCidade)

    limpa()

    escreva("Infome o nome da sua cidade em que você estuda: ")
    leia(cidadeEscola)

    limpa()

    escreva("A sua cidade é: " + nomeCidade + " e você estuda em: " + cidadeEscola)
  }
}
